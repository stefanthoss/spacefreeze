<h1>Listing spaces</h1>

<table>
  <tr>
    <th>User</th>
    <th>Begin</th>
    <th>End</th>
    <th>Price</th>
    <th>Search</th>
    <th>Address</th>
    <th>Lat/Lon</th>
    <th>Space type</th>
    <th>Charge</th>
    <th>Picture</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @spaces.each do |space| %>
  <tr>
    <td><%= space.user_id %></td>
    <td><%= space.begin %></td>
    <td><%= space.end %></td>
    <td><%= space.price %></td>
    <td><%= space.search_id %></td>
    <td><%= space.address %></td>
    <td><%= space.lat %> / <%= space.lon %></td>
    <td><%= space.space_type %></td>
    <td><%= space.charge %></td>
    <td><%= space.picture %></td>
    <td><%= link_to 'Show', space %></td>
    <td><%= link_to 'Edit', edit_space_path(space) %></td>
    <td><%= link_to 'Destroy', space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Space', new_space_path %>
