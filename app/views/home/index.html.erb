<div class="navbar">
  <div class="navbar-inner">
	<a class="brand">Spacefreeze</a>
	<div class="nav pull-right">
		<a class="btn btn-primary"  id="log_in" role="button" >Log In</a>
		<a class="btn btn-success"  id="sign_up" role="button" >Sign Up</a>
		<a class="btn btn-warning"  id="list_my_space" role="button" >List My Space</a>			 					
	</div>
  </div>
</div>

<div class="span5 offset1">
	<input type="text" data-provide="typeahead" id="space_search"><a class="btn btn-info"  role="button" id='search_text'>Search<i class="icon-search"></i></a>
</div>

<div class="row-fluid">
	<div class="span10 offset1">

		<div class="tab-content row-fluid" >
			<div class="tab-pane active" id="home_image_tab">	
					<%= image_tag("background-sf.png") %>
							
			</div>
		</div>				

		<table class="table">
			<thead>
				<tr>
					<th>Space For Your Car</th>
					<th>Space For Your Toys</th>
					<th>Space For Your Things</th>	
				</tr>
			</thead>
			<tbody>
				<tr>
					<td><%= image_tag("car.png") %></td>
					<td><%= image_tag("boat.png") %></td>
					<td><%= image_tag("moving.png") %></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
		