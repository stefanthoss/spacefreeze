<h1>Listing searches</h1>

<table>
  <tr>
    <th>User</th>
    <th>Address</th>
    <th>Begin</th>
    <th>End</th>
    <th>Max distance</th>
    <th>Max price</th>
    <th>Space type</th>
    <th>Charge</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @searches.each do |search| %>
  <tr>
    <td><%= search.user_id %></td>
    <td><%= search.address %></td>
    <td><%= search.begin %></td>
    <td><%= search.end %></td>
    <td><%= search.max_distance %></td>
    <td><%= search.max_price %></td>
    <td><%= search.space_type %></td>
    <td><%= search.charge %></td>
    <td><%= link_to 'Show', search %></td>
    <td><%= link_to 'Edit', edit_search_path(search) %></td>
    <td><%= link_to 'Destroy', search, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Search', new_search_path %>
